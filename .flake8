[flake8]
max-line-length = 88
ignore = E203, E231, E501, W503, F403, E265, W293,E711,F601
exclude =
    ./backend/.git  
    ./backend/__pycache__ 
    ./backend/migrations
    ./backend/venv
    ./backend/.venv
    ./backend/env
    ./backend/.env
    ./backend/.idea
    ./backend/.github
    ./frontend